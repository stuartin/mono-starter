{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ESNext"
        ],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "isolatedModules": true,
        "skipLibCheck": true,
        "strict": true,
        "plugins": [
            {
                "name": "gql.tada/ts-plugin",
                "schema": "http://127.0.0.1:8787/graphql",
                "tadaOutputLocation": "./graphql/graphql-env.d.ts"
            }
        ]
    }
}
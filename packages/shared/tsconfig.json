{
    "include": [
        "**/*.ts",
    ],
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ESNext"
        ],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "isolatedModules": true,
        "skipLibCheck": true,
        "strict": true
    }
}